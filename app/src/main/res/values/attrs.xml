<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleBar">
        <attr name="textTitle" format="string" />
        <attr name="barBg" format="color" />
        <attr name="titleColor" format="color" />
        <attr name="backImg" format="integer|reference"/>
        <attr name="TitleSrc" format="integer|reference"/>
        <attr name="backTextColor" format="color|reference"/>
        <attr name="rightText" format="string"/>
        <attr name="rightIcon" format="integer|reference"/>
        <attr name="hasRightIcon" format="boolean"/>
        <attr name="hasIvTitle" format="boolean"/>
        <attr name="hasLine" format="boolean"/>
        <attr name="hasTvBack" format="boolean"/>
        <attr name="tvBack" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="EasyStatusView">
        <attr name="esv_loading" format="reference"/>
        <attr name="esv_error" format="reference"/>
        <attr name="esv_empty" format="reference"/>
        <attr name="esv_noNet" format="reference"/>
    </declare-styleable>


</resources>